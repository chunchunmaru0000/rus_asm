вход глав

участок чит изм
	а_вот_не_текст_а_метка:
	пусть текст0 байт \
	"эээ"10
	пусть байт "негры"10
	пусть текст1 байт "эээ
негры
"
	;пусть вещ0 байт 0.1
	пусть вещ1 чбайт -0.1
	пусть вещ2 \
		вбайт 0.1 \
		байт "here its text"10
	пусть число0 вбайт 0хееее_697568_ееееее
	пусть число1 вбайт 0б1111111100000000_1111111100000000_1111111100000000_1111111100000000
	пусть число3 вбайт 0хабв_где
	пусть число4 вбайт 0хабс_тиф
	пусть текст2 байт 9"some text in english"10 9"to see it in executable"10 ;1.23
участок чит исп
глав:
	быть еах чбайт "Z"
	быть еах чбайт "э"
	быть рах чбайт вбайт чбайт 1
	быть рди чбайт 1
	быть рси вбайт а_вот_не_текст_а_метка
	быть рдх 18
	сзов

	быть рах вбайт 1
	быть рси текст1
	сзов

	быть рах вбайт 1
	быть рси текст2
	быть рдх 47
	сзов

завершить:
	быть рах чбайт "<"
	идти нааааа
	быть рди 124
куда_надо:
	сзов
нааааа:
	быть рди вбайт 77
	идти куда_надо
	быть рди 71
	идти куда_надо
