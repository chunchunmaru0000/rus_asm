вход глав

участок чит изм

; ##############################################################
пусть перевод байт 10
пусть точки байт "⠏⠛⠹⠼⠶⠧"
вот ТОЧЕК 6
вот РАЗМЕР_ТОЧКИ 3
пусть точка чбайт 0

; ##############################################################
пусть перевод_куда байт 0x1b "[D"
вот длина_куда 3

; ##############################################################
пусть секунд вбайт 0 100_000_000

; ##############################################################
участок чит исп
глав:
	искл ебх ебх
	зов перевести_строку
цикл_вывода:

	зов вывод_точки

	плюс чбайт(точка) 1
	быть еах (точка)
	срав еах ТОЧЕК
	сбытьбр еах ебх
	быть (точка) еах
	
	зов перевести_строку_куда
	зов спать

идти цикл_вывода

вывод_точки:
	быть рси точки
	быть еах (точка)
	быть едх РАЗМЕР_ТОЧКИ
	зумн еах едх
	плюс рси рах ; теперь тут адрес точки

	быть еах 1
	быть еди 1
	быть едх РАЗМЕР_ТОЧКИ ; два байта символ
	сзов

	возд

спать:
	быть еах 35
	быть рди секунд
	искл рси рси
	сзов
	возд

перевести_строку:
	быть еах 1
	быть еди 1
	быть рси перевод
	быть едх 1
	сзов
	возд
	
перевести_строку_куда:
	быть еах 1
	быть еди 1
	быть рси перевод_куда
	быть едх длина_куда
	сзов
	возд

