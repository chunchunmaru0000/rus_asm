вход start

участок чит исп

start:
    ; Подготовка данных
    бууо э0 эбайт(data1)
    бууо э1 эбайт(data2)
    бууо э2 эбайт(data3)
    бууо э3 эбайт(data4)
    бытьчвр э4 эбайт(data1)
    бытьчвнр э5 эбайт(data2)

    ; SSE1 инструкции
    бмзуо еах э0
    крньуо э0 э1
    окрньуо э1 э2
    оуо э2 э3
    иуо э3 э0
    иуо э0 э1
    илиуо э1 э2
    исклуо э2 э3
    плюсуо э3 э0
    умнуо э0 э1
    минсуо э1 э2
    минумо э2 э3
    делуо э3 э0
    максуо э0 э1

    ; SSE2 инструкции
    бмзуд ебх э1
    крньуд э1 э2
    иуд э2 э3
    инуд э3 э0
    илиуд э0 э1
    исклуд э1 э2
    плюсуд э2 э3
    умнуд э3 э0
    минсуд э0 э1
    минмуд э1 э2
    делуд э2 э3
    максуд э3 э0

    ; Конверсии
    преуо2уд э0 э1
    преуд2уо э1 э2
    пречч2уо э2 э3
    преуо2чч э3 э0
    преоуо2чч э0 э1

    ; Скалярные операции
    крньсо э1 чбайт(data1)
    окрньсо э2 чбайт(data2)
    осо э3 чбайт(data3)
    плюссо э0 э1
    умнсо э1 э2
    минссо э2 э3
    минмсо э3 э0
    делсо э0 э1
    макссо э1 э2

    крньсд э2 вбайт(data3)
    плюссд э3 э0
    умнсд э0 э1
    минссд э1 э2
    минмсд э2 э3
    делсд э3 э0
    макссд э0 э1

    ; Упаковка/распаковка
    урспнбд э0 э1
    урспндч э1 э2
    урспнчв э2 э3
    уупкзндб э3 э0
    усраввб э0 э1
    усраввд э1 э2
    усраввч э2 э3
    уупкбндб э3 э0
    урспвбд э0 э1
    урспвдч э1 э2
    урспвчв э2 э3
    уупкзнчд э3 э0
    урспнвдв э0 э1
    урспввдв э1 э2

    ; Shuffle и сдвиги
    утаснд э0 э1 0x1E
    утасвд э1 э2 0x4B
    утасч э2 э3 0xE1

    усплд э0 3
    успад э1 2
    усллд э2 1
    усплч э0 4
    успач э1 3
    усллч э2 2
    усплв э0 5
    усллв э1 1
    усплдв э2 3
    усллдв э3 2

    ; Сравнения
    усраврб э0 э1
    усраврд э1 э2
    усраврч э2 э3

    ; Горизонтальные операции
    гплюсуд э0 э1
    гплюсуо э1 э2
    гминсуд э2 э3
    гминсуо э3 э0

участок чит изм
    пусть data1 чбайт 1.0 2.0 3.0 4.0
    пусть data2 вбайт 1.5 2.5
    пусть data3 чбайт 5.0 6.0 7.0 8.0
    пусть data4 вбайт 3.5 4.5

