вход start

участок чит изм
    пусть mem1 байт 0
    пусть mem2 байт 0
    пусть vал1 чбайт 1234
    пусть vал2 чбайт 5678

участок чит исп
start:

    ; SETcc пробввдg
    быть еах 5
    быть ебх 10
    срав еах ебх  ; ZF=0 CF=1 SF=1

    ; Все варианты SETcc в регистр и в память
    усти     ал
    устни    бл
    устз     сл
    устнз    дл
    уст0     байт (mem1)
    устн0    байт (mem2)
    устп     ал         ; CF=1
    уствр    бл         ; CF=0
    устпр    сл         ; CF=1 || ZF=1
    уств     дл         ; CF=0 && ZF=0
    устм     ал         ; SF!=OF
    устбр    бл         ; SF==OF
    устмр    сл         ; ZF=1 || SF!=OF
    устб     дл         ; ZF=0 && SF==OF

    ; CMOVcc пробввдg
    быть еах (vал1)
    быть ебх (vал2)
    срав еах ебх  ; еах < ебх

    ; Подготовим регистры и память
    быть есх 1111
    быть едх 2222
    быть (vал1) есх
    быть (vал2) едх

    ; Все CMOVcc из памяти и регистров
    быть еах 0
    сбытьи  еах есх
    сбытьни еах (vал1)
    сбытьз  еах едх
    сбытьнз еах (vал2)
    сбыть0  еах есх
    сбытьн0 еах (vал1)
    сбытьп  еах едх
    сбытьвр еах (vал2)
    сбытьпр еах есх
    сбытьв  еах (vал1)
    сбытьм  еах едх
    сбытьбр еах (vал2)
    сбытьмр еах есх
    сбытьб  еах (vал1)
