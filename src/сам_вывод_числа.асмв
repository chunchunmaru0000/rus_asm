    толк рсх
    толк рдх
    толк рди
    толк рси
    быть рди buf
    быть рсх LEN
    слear_buf:
        быть байт (рди) 32
        увлч рди
        умнш рсх
        идн0 слear_buf

    быть рди buf
    плюс рди LEN
    умнш рди

    быть рсх 10 ; умншimал system so devide рах by 10
    быть рах р8
    prпрер_сгar:
        искл рдх рдх
        дел рсх

        плюс рдх 48 ; прер to сгar
        быть байт (рди) дл

        умнш рди
        срав рах 0
        иднр prпрер_сгar

    быть рах 1
    быть рди 2
    быть рси buf
    быть рдх 17
