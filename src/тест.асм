вход глав


; исп - исполняемый - executable - 1
; изм - изменяемый - writeable - 2
; чит - читаемый - readable - 4
участок чит исп
глав:
	быть рах 1 ; рах = 1
	быть рди 1 ; рди = 1
	;быть вбайт(рси) 7599094713931097193 ; вбайт(рси) = текст или же (рси) = вбайт текст
	быть рси 0 ; текст ; пока так для простоты парсера
	; быть байт (рси - 4) 123  ; и так быть должно достаточно просто вмето [] пусть будут ()
	быть рдх 7 ; рдх = 7
	сзов

	быть рах 60 ;рах = 60
	быть рди 0 ;рди = 0
	сзов

участок чит изм
участок 7
;	текст байт "эээ",10,0
;
;0ха0
;0хА0
;0ХА0
;0Б1000
