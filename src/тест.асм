вход глав

участок чит исп
пусть текст0 байт \
	"эээ"10 \
	"негры"10
пусть текст1 байт "эээ
негры
"
пусть число0 вбайт 12345678
пусть текст2 байт "some text in english to see it in executable"

глав:
	быть еах 1
	быть еди 1
	быть еси текст0
	быть едх 18
	сзов

	быть еах 1
	быть еди 1
	быть еси текст1
	быть едх 18
	сзов

завершить:
	быть еах 60
	идти нааааа
	быть еди 124
куда_надо:
	сзов
нааааа:
	быть еди 77
	идти куда_надо
	быть еди 71
	идти куда_надо
