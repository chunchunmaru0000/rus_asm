счет ЛРод (
	; Слово
	Слово,
	; Другое
	Перевод, ; \n
	Дветоч,	 ; :
	Заметка, ; комментарий
	Черта,	 ; \
	Зап,	 ; ,
	Раздел,	 ; // разделитель
	ЛСкоб,	 ; (
	ПСкоб,	 ; )
	КцФ,		; конец файла
	; Литералы
	Чсл,
	Вещ,
	Стр,
	; Арифметика
	Увлч,	; ++
	Умнш,	; --
	Равно,	; =
	Плюс,	; +
	Минус,	; -
	Умн,	; *
	Дел,	; /
)

лик ВФайле (
	путь текст: стр
	длина: ц64
)

лик Исток (
	строка буква: ц32
	в_файле: лик ВФайле
)

лик Лексема (
	вид: стр
	род: счет ЛРод
	ист: лик Исток
	ч/чсл: ч64, в/вещ: в64, с/стр: стр
)
#вот Лексемы Лексема

пусть ЦЧ: стр = "\x1B[30m",		; черный
пусть ЦТ: стр = "\033[90m",		; темный(серый)
пусть ЦР: стр = "\x1B[91m",		;розовый
пусть ЦК: стр = "\x1B[31m",		; красный
пусть ЦЗ: стр = "\x1B[32m",		; зеленый
пусть ЦЖ: стр = "\x1B[33m",		; желтый
пусть ЦС: стр = "\x1B[34m",		; синий
пусть ЦФ: стр = "\x1B[35m",		; фиолетовый
пусть ЦФС: стр = "\033[95m",	; фиолетовый светлый
пусть ЦГ: стр = "\x1B[36m",		; голубой
пусть ЦБ: стр = "\x1B[37m",		; белый
пусть ЦН: стр = "\x1B[0m",		; никакой
пусть ОТСТУП : стр = "    ",
